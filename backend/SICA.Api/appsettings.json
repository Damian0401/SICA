{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Api": {
    "FilesCollectionName": "files",
    "FilesContainerName": "files",
    "DefaultAcceptLanguage": "en-US"
  },
  "VectorStore": {
    "QdrantUrl": "<qdrant-url>",
    "QdrantApiKey": "<qdrant-api-key>",
    "EmbeddingModelUrl": "<embedding-model-url>",
    "EmbeddingModelId": "nomic-embed-text",
    "EmbeddingModelVectorSize": 768
  },
  "BlobStore": {
    "ConnectionString": "<blob-connection-string>"
  },
  "TextExtraction": {
    "TesseractDataPath": "./Resources"
  }
}
